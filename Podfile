  pod 'GoogleAPIClientForREST/Gmail', '~> 1.0.3'
  pod 'GTMOAuth2', '~> 1.1.2'
  pod 'Google/SignIn'
  pod 'SwiftyBase64', '~> 1.0'

  target 'ipadTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ipadUITests' do
    inherit! :search_paths
    # Pods for testing
  end



end
